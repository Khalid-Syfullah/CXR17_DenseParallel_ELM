<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cxr17_denseparallel_elm">

    <application
        android:allowBackup="true"
        android:icon="@drawable/circle"
        android:label="@string/app_name"
        android:roundIcon="@drawable/circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.CXR17_DenseParallel_ELM">
        <activity
            android:name=".XRayActivity"
            android:screenOrientation="nosensor" />

        <activity
            android:name=".MainActivity"
            android:screenOrientation="nosensor" />

        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CXR17_DenseParallel_ELM.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>